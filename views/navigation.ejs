<!DOCTYPE html>
<body>
    <link
    rel="stylesheet"
    type="text/css"
    href="../assets/stylesheets/styles.css"
  />
<%- include('partials/header.ejs'); %>
<html>
  <head>
    <title>Simple Map</title>
    <meta name="viewport" content="initial-scale=1.0">
    <meta charset="utf-8">
    <style>
      #map {
        height: 60%;
      }
      html, body {
        height: 100%;
        margin: 20;
        padding: 50;
      }
    </style>
  </head>
  <body>
    <div id="map"></div>
    <script>
      var map;
      function initMap() {
        map = new google.maps.Map(document.getElementById('map'), {
          center: {lat: 35.227, lng: -80.843},
          zoom: 11
        });
      }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDxr-EALQsVDsjYqu6cYeW7KJx4Iz-IF_Y&callback=initMap"
    async defer></script>
    <%- include('partials/footer.ejs'); %>
  </body>
</body>
</html>